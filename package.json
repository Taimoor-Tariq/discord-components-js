{
    "name": "discord-interaction-components",
    "version": "1.1.2",
    "description": "Discord interaction component generator.",
    "main": "dist/index.js",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "publish:npm": "node ./scripts/publish-npm.js && npm publish",
        "publish:github": "node ./scripts/publish-github.js && npm publish",
        "publish:all": "npm run publish:github && npm run publish:npm"
    },
    "devDependencies": {
        "typescript": "^4.6.2"
    },
    "repository": {
        "url": "https://github.com/Taimoor-Tariq/discord-interaction-components.git"
    },
    "bugs": {
        "url": "https://github.com/Taimoor-Tariq/discord-interaction-components/issues"
    },
    "homepage": "https://github.com/Taimoor-Tariq/discord-interaction-components",
    "files": [
        "dist/**/*"
    ],
    "prepare": "yarn run build",
    "types": "dist/index.d.ts",
    "keywords": [
        "discord",
        "discord components",
        "discord.js",
        "discord.js components"
    ]
}